{
  "name": "progress-4gl-formatter",
  "displayName": "Progress 4GL Formatter",
  "description": "Formatter for Progress 4GL (OpenEdge ABL).",
  "version": "0.0.4",
  "icon": "logo.png",
  "publisher": "GabrielCoimbra",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:progress"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "progress",
        "aliases": [
          "Progress",
          "OpenEdge ABL"
        ],
        "extensions": [
          ".p",
          ".w",
          ".i",
          ".cls"
        ]
      }
    ],
    "formatters": [
      {
        "language": "progress",
        "displayName": "Progress 4GL Formatter",
        "id": "progress-4gl-formatter"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coimbrox/progress-4gl-formatter"
  }
}